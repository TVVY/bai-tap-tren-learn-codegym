   "source": [
    "import turtle\n",
    "n = int(input(\"Nhập số bán kính: \"))\n",
    "t = turtle.Turtle()\n",
    "d = 0.1\n",
    "while True:\n",
    "    t.forward(d)\n",
    "    t.left(10)\n",
    "    d += 0.1\n",
    "    if t.distance(0, 0) >=n:\n",
    "        break\n",
    "\n",
    "turtle.done()"
   ]
